{
  "name": "module-references",
  "version": "0.0.2",
  "author": "Joris van der Wel <joris@jorisvanderwel.com>",
  "description": "Annotate file references within the source code of your node packages. These references can be scanned for using the dependency graph of a module. This package takes input from module-deps and generates a list of all the references.",
  "keywords": [
    "annotate",
    "dependency",
    "graph",
    "browser",
    "require",
    "module",
    "exports",
    "source",
    "analyze",
    "ast",
    "dummy",
    "asset",
    "static",
    "file",
    "preprocess",
    "merge",
    "meta",
    "metadata",
    "compiler",
    "reference",
    "resource",
    "static-reference",
    "link",
    "relationship"
  ],
  "bugs": {
    "url": "https://github.com/Joris-van-der-Wel/node-module-references/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Joris-van-der-Wel/node-module-references.git"
  },
  "main": "./lib/ModuleReferencesStream.js",
  "directories": {
    "lib": "./lib"
  },
  "dependencies": {
    "detective": "^3.1.0",
    "inherits": "^2.0.1",
    "q": "^2.0.2",
    "q-io": "^1.11.4",
    "readable-stream": "^1.0.27-1",
    "resolve": "^1.0.0",
    "static-reference": "^1.0.0"
  },
  "devDependencies": {
    "istanbul": ">=0.3.1",
    "jison": "0.4.13",
    "jshint": ">=2.5.3",
    "nodeunit": ">=0.9.0"
  },
  "scripts": {
    "test": "istanbul cover ./node_modules/nodeunit/bin/nodeunit test/ModuleReferencesStream.js test/FilterParser.js",
    "prepublish": "node lib/filter-generate.js > lib/FilterParser.js"
  },
  "jshintConfig": {
    "node": true,
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": false,
    "freeze": true,
    "immed": true,
    "indent": 8,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "nonbsp": true,
    "nonew": true,
    "quotmark": "single",
    "undef": true,
    "unused": "vars",
    "strict": true,
    "laxbreak": true,
    "-W064": true
  }
}
